   "source": [
    "paragraph = \"the quick brown fox jumps over the lazy dog\"\n",
    "words = paragraph.split(\" \")\n",
    "print(words)\n",
    "\n",
    "word_count = dict()\n",
    "\n",
    "for word in words:\n",
    "    keys = word_count.keys()\n",
    "    if word in keys:\n",
    "        word_count[word] += 1\n",
    "    else:\n",
    "        word_count[word] = 1\n",
    "\n",
    "print(word_count)"
